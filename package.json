{
  "name": "play-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile:circuits && npm run setup && npm run compile",
    "compile": "hardhat compile && typechain --out-dir types --target ethers-v5 './artifacts/contracts/**/!(*.dbg).json'",
    "compile:circuits": "mkdir -p build && circom circuits/withdraw.circom --r1cs --wasm -o build",
    "setup": "sh quickSetup.sh",
    "info": "snarkjs info -r build/withdraw.r1cs",
    "test": "hardhat test tests/*",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@openzeppelin/contracts": "^3.4.2",
    "@typechain/ethers-v5": "^7.2.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.11.13",
    "chai": "^4.3.4",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.0",
    "mocha": "^9.1.3",
    "prettier": "2.3.2",
    "circomlib": "^2.0.2",
    "circomlibjs": "0.1.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "snarkjs": "^0.4.13",
    "ts-node": "^10.4.0",
    "typechain": "^5.2.0",
    "typescript": "^4.5.4"
  }
}
